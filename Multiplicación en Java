public class E011MultiplicacionMas4 {
     public static void main(String[] args) {
        System.out.println("Ingrese los numeros que quiera sumar");
        //Scanner
        Scanner teclado = new Scanner(System.in);
        int numero1,numero2,multiplicacion;
        System.out.println("Ingrese el pimer numero a operar");
        numero1 = teclado.nextInt();
        System.out.println("Ingrese el segundo numero a operar");
        numero2= teclado.nextInt();
        multiplicacion = (numero1*numero2)+4;
         System.out.println("El resultado de la operacion es: "+multiplicacion);
}
}
